    <script src="/js/loading.js"></script>

</head>

<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-10">
                <button id="load" class="btn btn-primary">Load 10 Blocks</button>
            </div>
        </div>
        <div class="row errorRow">
            <p class="errorText"></p>
        </div>
        <div class="row tableRow">
            <table class="table table-hover">
                <thead>
                    <th>Id</th>
                    <th>Timestamp</th>
                    <th>Count of Actions</th>
                </thead>
                <tbody id="block">

                </tbody>
            </table>
        </div>
    </div>
    <script src="/js/app.js"></script>
